# ====== Global ======
COMPOSE_PROJECT_NAME=halai
TZ=Europe/Rome

# ====== Traefik / Networking ======
TRAEFIK_ACME_EMAIL=your-email@example.com
TRAEFIK_DOMAIN=halai.local
TRAEFIK_DASHBOARD_DOMAIN=traefik.halai.local
TRAEFIK_LOG_LEVEL=INFO

# ====== PostgreSQL ======
POSTGRES_USER=n8n
POSTGRES_PASSWORD=change_me
POSTGRES_DB=n8n

# ====== Redis ======
REDIS_PASSWORD=

# ====== n8n ======
N8N_HOST=halai.local
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_ENCRYPTION_KEY=super-secret-key
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin
N8N_JWT_SECRET=super-secret-jwt
N8N_EDITOR_BASE_URL=https://halai.local/
N8N_API_BASE_URL=https://halai.local/
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=0

# ====== Ollama / Open WebUI ======
OPEN_WEBUI_DOMAIN=openwebui.halai.local
OLLAMA_GPU=false

# ====== ComfyUI ======
COMFYUI_DOMAIN=comfyui.halai.local
